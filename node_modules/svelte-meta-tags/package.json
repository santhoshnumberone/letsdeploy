{
  "name": "svelte-meta-tags",
  "version": "2.5.0",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "dependencies": {
    "schema-dts": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@sveltejs/adapter-auto": "1.0.0-next.33",
    "@sveltejs/kit": "1.0.0-next.301",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "cypress": "9.5.2",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-svelte3": "3.4.1",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "prettier-plugin-svelte": "2.6.0",
    "semantic-release": "19.0.2",
    "svelte": "3.46.4",
    "svelte-check": "2.4.6",
    "svelte-preprocess": "4.10.4",
    "svelte2tsx": "0.5.6",
    "tslib": "2.3.1",
    "typescript": "4.6.2"
  },
  "peerDependencies": {
    "svelte": "^3.39.0"
  },
  "lint-staged": {
    "*.{js,ts,cjs,svelte,md,html,css,json,yml,yaml}": "pnpm format",
    "*.{js,ts,svelte}": [
      "pnpm lint",
      "pnpm check"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        2,
        "always",
        200
      ]
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "package"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      "main"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    "./JsonLd.svelte": "./JsonLd.svelte",
    "./MetaTags.svelte": "./MetaTags.svelte",
    ".": "./index.js"
  },
  "svelte": "./index.js"
}
